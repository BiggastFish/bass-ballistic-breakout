// Handle Intro
if (!global.frozen)
{
    // Starting the intro animation
    if (startIntro)
    {
        startIntro = false;
        isIntro = true;
        drawBoss = true;
    }
    else if (isIntro)
    {
        bzztTimer++;
        if (bzztTimer = 30)
        {
            y = view_yview - 96;
            yspeed = 4;
            grav = -(yspeed * yspeed) / (2 * abs(y - ystart));
            y+= 3;
            r = instance_create(x, y, objBuzzshockWings);
            r.depth = depth + 10;
            r.parent = id;
            l = instance_create(x, y, objBuzzshockWings);
            l.depth = depth - 10;
            l.offset = 2;
            l.parent = id;
            visible = 1;
        }
        if (y &gt;= ystart &amp;&amp; bzztTimer &gt;= 45)
        {
            if (bzztTimer &lt; 600) bzztTimer = 600;
            y = ystart;
            yspeed = 0;
            grav = 0;
            blockCollision = 0;
        }
        if (bzztTimer = 630)
        {
            bzztTimer = 0;
            isIntro = false;
            playSFX(sfxMechaDragon);
        }
    }
}
if (entityCanStep())
{
    if (isFight)
    {
        bzztTimer++;
        if (instance_exists(target)) 
            tx = target.x;
        if (!instance_exists(objBuzzshockWings))
        {
            
        }
        if (phase &lt; 4)
        {
            if (floor(blastImg) &gt; 0) 
                blastImg-= 1/3;
            else 
                blastImg = 0;
        }
        switch (phase)
        {
            case 0: // plug stream
                if (bzztTimer = 30)
                {
                    image_index = 1;
                    playSFX(sfxCommandoBombLaunch);
                    for (i = -2; i &lt; 2; i++)
                    {
                        if (i != 0)
                        {
                            m = instance_create(x + (88 * image_xscale) + 
                            (-16 * (i &lt; 0)) + (16 * (i &gt; 0)), y + 58, 
                            objTrailEffect);
                            m.sprite_index = sprElecnSpark;
                            m.hspeed = i * 1;
                            m.vspeed = -1 + (-1 * !(i % 2 = 0));
                            m.image_alpha = 1;
                            m.depth = depth - 20;
                        }
                    }
                }
                if (bzztTimer &gt;= 60 &amp;&amp; bzztTimer % 5 = 0 &amp;&amp; shotsDone &lt; 14)
                {
                    playSFX(sfxElecnShoot);
                    s = instance_create(x + (98 * image_xscale), y + 70, 
                    objChargingChesterSpark);
                    s.shotFrom = 1;
                    s.yspeed = 3;
                    s.dir = -image_xscale;
                    shotsDone++;
                }
                if (shotsDone = 14)
                {
                    shotsDone = 0;
                    bzztTimer = 0;
                    image_index = 0;
                    phase = 1;
                }
                break;
            case 1: // wiggle up and parabola
                if (bzztTimer = 30)
                {
                    middlePoint = x;
                    xVal = 90;
                    yspeed = -(64/240);
                }
                if (bzztTimer &gt; 30 &amp;&amp; !shotsDone)
                {
                    if (y &gt; view_yview + 16)
                    {
                        xVal-= (3 * image_xscale);
                        x = middlePoint + round(cos(degtorad(xVal)) * 32);
                    }
                    else
                    {
                        yspeed = 0;
                        xVal = 0;
                        bzztTimer = 0;
                        if (shotsDone &lt; 1) shotsDone = 1;
                    }
                    var dist = abs(x + (78 * image_xscale) - tx);
                    if (dist &lt;= 24)
                        cannonImg = 1;
                    else
                    {
                        if (tx &lt; x + (78 * image_xscale))
                        {
                            if (image_xscale &gt; 0)
                                cannonImg = 0;
                            else
                                cannonImg = 2;
                        }
                    }
                    if (bzztTimer &gt;= 90 &amp;&amp; bzztTimer % 30 = 0)
                    {
                        playSFX(sfxElectricShot);
                        with instance_create(x + 78 * image_xscale, y + 44, 
                        objEnemyBullet)
                        {
                            sprite_index = sprElecitShot;
                            image_speed = .4;
                            if (instance_exists(target)) aimAtTarget(2);
                            else yspeed = 2;
                        }
                        blastImg = 5;
                    }
                }
                if (shotsDone)
                {
                    if (bzztTimer = 50)
                    {
                        var airtime, ySpd, xEnd, xStart, xDisplace;
                        grav = -.17;
                        ySpd = -sqrt(abs(2*.2*96));
                        yspeed = -ySpd
                        airtime = abs(2*yspeed/.17);
                        xEnd = x - (112 * image_xscale);
                        xStart = x;
                        xDisplace = xEnd - xStart;
                        xspeed = xDisplace / airtime;
                    }
                    if (bzztTimer &gt;= 30 &amp;&amp; bzztTimer % 30 = 0)
                    {
                        if (instance_number(objRingRing) &lt; 3)
                        {
                            r = instance_create(x + (36 * image_xscale), y + 64,
                            objRingRing);
                            r.respawn = 0;
                            r.itemDrop = -1;
                        }
                    }
                }
                if (bbox_bottom &lt; view_yview - 16)
                {
                    xspeed = 0;
                    yspeed = 0;
                    grav = 0;
                    shotsDone = 0;
                    bzztTimer = 0;
                    phase = 2;
                    x = middlePoint;
                    image_xscale*=-1;
                    y = view_yview - 96;
                }
                break;
            case 2: // whoosh
                if (bzztTimer = 30)
                {
                    yspeed = 4;
                    grav = -(yspeed * yspeed) / (2 * abs(y - ystart));
                    y+= 3;
                }
                if (bzztTimer &gt; 30 &amp;&amp; (yspeed &lt;= 0 || y &gt;= ystart))
                {
                    y = ystart;
                    grav = 0;
                    yspeed = 0;
                    phase = 3;
                    bzztTimer = 0;
                }
                break;
            case 3: // spark and lightning
                if (bzztTimer = 30 || bzztTimer = 150 || bzztTimer = 270)
                {
                    playSFX(sfxDoor);
                    image_index = 1;
                }
                if (bzztTimer = 50 || bzztTimer = 170 || bzztTimer = 290)
                    image_index = 0;
                if ((bzztTimer &gt;= 40 &amp;&amp; bzztTimer &lt;= 50 || 
                bzztTimer &gt;= 160 &amp;&amp; bzztTimer &lt;= 170 ||
                bzztTimer &gt;= 280 &amp;&amp; bzztTimer &lt;= 290) &amp;&amp; 
                bzztTimer % 5 = 0)
                {
                    playSFX(sfxElecnShoot);
                    s = instance_create(x + (98 * image_xscale), y + 70, 
                    objChargingChesterSpark);
                    s.shotFrom = 1;
                    s.yspeed = 3;
                    s.dir = -image_xscale;
                }
                if (bzztTimer = 60 || bzztTimer = 180 || bzztTimer = 300)
                {
                    playSFX(sfxAirShooter);
                    s = instance_create(x + (68 * image_xscale), y + 15,
                    objBuzzshockSpark);
                    s.depth = depth + 5;
                    s.dir = -image_xscale;
                    s.yMov = y - 18;
                }
                if (bzztTimer &gt;= 300 &amp;&amp; !instance_exists(objBuzzshockSpark))
                {
                    bzztTimer = 0;
                    phase = 0;
                } 
                break;
            case 4: //moving to center of room
                x = xVal + round(cos(degtorad(irandom(360))) * 4);
                y = middlePoint + round(sin(degtorad(irandom(360))) * 4);
                visible = choose(0, 0, 1);
                if (bzztTimer % 4 = 0)
                {
                    playSFX(sfxExplosion);
                    instance_create(x + (54 * image_xscale) + 
                    irandom_range(-16, 16), y + 41 + irandom_range(-16,16),
                    objExplosion);
                }
                if (bzztTimer = 30)
                {
                    var xscaleOff;
                    if (image_xscale &gt; 0)
                        xscaleOff = -48;
                    else
                        xscaleOff = 48;
                    blastImg = abs(xVal - (view_xview + 128 + xscaleOff)) / 60;
                    if (xVal &gt; view_xview + 128 + xscaleOff)
                        blastImg = -blastImg;
                    shotsDone = abs(middlePoint - (view_yview + 48)) / 60;
                    if (middlePoint &gt; view_yview + 48)
                        shotsDone = -shotsDone;
                }
                if (bzztTimer &gt;= 30)
                {
                    xVal+= blastImg;
                    middlePoint+= shotsDone;
                }
                if (bzztTimer = 120)
                {
                    blastImg = 0;
                    shotsDone = 0;
                }
                if (bzztTimer = 180)
                {
                    with (objBuzzshockWings) instance_destroy();
                    for (i = -2; i &lt;= 2; i++)
                    {
                        if (i != 0)
                        {
                            m = instance_create(x, y, objEnemyBullet);
                            m.image_xscale = image_xscale;
                            if (i % 2 = 0)
                            {
                                m.sprite_index = sprBuzzshockWings;
                                m.image_index = 2 + i;
                                m.xspeed = 2 * i;
                            }
                            else
                            {
                                m.sprite_index = sprBuzzshock;
                                m.image_index = 4 - (2 * (i &gt; 0));
                                m.xspeed = (2 * image_xscale) * i;
                            }
                            m.yspeed = -3;
                            m.grav = .2;
                            m.contactDamage = 0;
                        }
                    }
                    playSFX(sfxExplosion2);
                    event_user(EV_DEATH);
                }
        }
    }
}
