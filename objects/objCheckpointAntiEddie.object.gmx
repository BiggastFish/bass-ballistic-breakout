            <kind>1</kind>
            <string>if !global.frozen
{
    if (insideSection(x, y))
    {
        visible = 1;
        if (!touch)
        {
            animationLoop(0, 1, .05);
            if (place_meeting(x, y, objMegaman))
            {
                playSFX(sfxElementGrab);
                event_perform_object(objCheckpoint, ev_other, ev_user0);
                touch = 1;
                _im = 0;
                image_index = 2;
                yspeed = -2;
                if (!global.livesEnabled)
                {
                    e = instance_create(objMegaman.x, objMegaman.y, objLife);
                    e.visible = 0;
                }
            }
        }
        else if (touch == 1)
        {
            y += yspeed;
            yspeed += 0.25;
            if yspeed &gt;= 0
            {
                image_index = 3;
                if (y &gt;= ystart)
                {
                    touch = 2;
                    y = ystart;
                    image_index = 4;
                    playSFX(sfxLand);
                }
            }
        }
        else if (touch == 2)
        {
            animationLoop(4, 5, .05);
        }
    
        if (instance_exists(objMegaman))
        {
            calibrateDirection(objMegaman);
        }
        else
        {
            if (touch != 1)
            {
                image_index = 6;
            }
        }
    }
    else
    {
        visible = 0;
        if (global.checkpointY == yy &amp;&amp; global.checkpointX == xx)
        {
            instance_destroy();
        }

    }
}

</string>
